{
	"name": "hams",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "prisma generate && vite build  && node scripts/update-vercel-config",
		"preview": "vite preview --port 5173",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"prisma": {
		"seed": "node --loader ts-node/esm --experimental-specifier-resolution=node prisma/seed.ts"
	},
	"devDependencies": {
		"@auth/core": "0.16.1",
		"@auth/sveltekit": "0.3.9",
		"@aws-sdk/client-s3": "3.525.0",
		"@prisma/client": "5.6.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-vercel": "^4.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/bcrypt": "5.0.2",
		"@types/node": "20.10.0",
		"@types/nprogress": "0.2.2",
		"@typescript-eslint/eslint-plugin": "6.13.1",
		"@typescript-eslint/parser": "6.13.1",
		"@zerodevx/svelte-toast": "0.9.5",
		"autoprefixer": "10.4.16",
		"bcrypt": "5.1.1",
		"chart.js": "4.4.1",
		"dayjs": "1.11.10",
		"eslint": "8.54.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte": "2.35.1",
		"nock": "13.5.3",
		"nprogress": "0.2.0",
		"postcss": "8.4.31",
		"prettier": "2.8.0",
		"prettier-plugin-svelte": "2.10.1",
		"prisma": "5.6.0",
		"svelte": "4.2.7",
		"svelte-chartjs": "3.1.5",
		"svelte-check": "3.4.3",
		"svelte-multiselect": "10.2.0",
		"svelte-printpdf": "1.0.2-metadata",
		"svelte-table": "0.6.1",
		"sveltekit-superforms": "1.11.0",
		"tailwindcss": "3.3.5",
		"ts-node": "10.9.1",
		"tslib": "2.6.2",
		"typescript": "5.3.2",
		"vite": "^5.0.0",
		"zod": "3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/s3-request-presigner": "3.525.0",
		"resend": "3.4.0"
	}
}
