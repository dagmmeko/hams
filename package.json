{
	"name": "hams",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "prisma generate && vite build  && node scripts/update-vercel-config",
		"preview": "vite preview --port 5173",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"sync": "svelte-kit sync"
	},
	"prisma": {
		"seed": "node --loader ts-node/esm --experimental-specifier-resolution=node prisma/seed.ts"
	},
	"devDependencies": {
		"@auth/core": "0.16.1",
		"@auth/sveltekit": "0.3.9",
		"@aws-sdk/client-s3": "3.758.0",
		"@aws-sdk/s3-request-presigner": "3.758.0",
		"@eslint/js": "9.21.0",
		"@prisma/client": "5.6.0",
		"@sveltejs/adapter-vercel": "5.6.3",
		"@sveltejs/kit": "2.17.3",
		"@sveltejs/vite-plugin-svelte": "5.0.3",
		"@types/bcrypt": "5.0.2",
		"@types/node": "^22",
		"@types/nprogress": "0.2.3",
		"@zerodevx/svelte-toast": "0.9.6",
		"autoprefixer": "10.4.20",
		"bcrypt": "5.1.1",
		"chart.js": "4.4.8",
		"dayjs": "1.11.13",
		"eslint": "9.21.0",
		"eslint-config-prettier": "10.0.2",
		"eslint-plugin-svelte": "3.0.2",
		"globals": "16.0.0",
		"nprogress": "0.2.0",
		"postcss": "8.5.3",
		"prettier": "3.5.3",
		"prettier-plugin-svelte": "3.3.3",
		"prettier-plugin-tailwindcss": "0.6.11",
		"prisma": "5.6.0",
		"resend": "3.5.0",
		"svelte": "5.21.0",
		"svelte-check": "4.1.4",
		"svelte-multiselect": "10.3.0",
		"svelte-printpdf": "1.0.2-metadata",
		"svelte-table": "0.6.3",
		"sveltekit-superforms": "1.13.4",
		"tailwindcss": "3.4.17",
		"ts-node": "10.9.2",
		"typescript": "5.8.2",
		"typescript-eslint": "8.26.0",
		"vite": "6.2.0",
		"zod": "3.24.2"
	},
	"type": "module"
}
